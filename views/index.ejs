        <script type="text/javascript">
            $(document).on('ready', function() { 
                    
                $(".lines-menu").slick({
                dots: false,
                infinite: false,
                centerMode: false,                
                slidesToShow: 4.7,
                slidesToScroll: 3,
                responsive: [
                    {
                        breakpoint: 480,
                        settings: {                            
                            centerMode: true,
                            centerPadding: '40px',
                            slidesToShow: 1
                        }
                    },
                    {
                        breakpoint: 768,
                        settings: {                            
                            centerMode: true,
                            centerPadding: '40px',
                            slidesToShow: 3
                        }
                    }                    
                ]
                
                }); 
                            

                function initializeSliders() {
                $( ".products" ).each(function( index ) {

                    $(this).slick({
                        dots: false,
                        infinite: false,
                        centerMode: false,                
                        slidesToShow: 3.4,
                        slidesToScroll: 3,
                        responsive: [
                            {
                                breakpoint: 768,
                                settings: {                            
                                    centerMode: true,
                                    // centerPadding: '40px',
                                    slidesToShow: 2.4,
                                    slidesToScroll: 2,
                                }
                            },
                            {
                                breakpoint: 480,
                                settings: {                            
                                    centerMode: true,
                                    // centerPadding: '40px',
                                    slidesToShow: 1,
                                    slidesToScroll: 1,
                                }
                            }
                        ]
                    }); 

                });

                }
                
                function resizeSliders() {

                
                setTimeout(function () { 
                    $( ".slider" ).each(function( index ) {
                        $(this).slick('setPosition');
                        $(this).slick('resize');     
                    });  
                    console.log("resizeSliders");
                }, 500);   
                
                }
        

                initializeSliders();


                $(window).resize(function() {
                    
                resizeSliders();          
                    
                });

                $( window ).on( "orientationchange", function( event ) {            
                location.reload();
                });


                setTimeout(function () {                  
                resizeSliders();
                }, 500);
                

                $('.nav-item a').on('click', function (e) {
                // e.stopPropagation();
                console.log("nav-item a");
                e.preventDefault();                
                $('.nav-link').removeClass('active');
                $(this).tab('show');                
                // initializeSliders();
                resizeSliders();
                });
        
                                            

            });
        </script>

